# ConnectMe - Environment variables for Docker Compose
# Copy to .env and adjust values.
# NB! Never commit .env to git.

# Spring profile for services (dev = show-sql, prod = no sql logging)
SPRING_PROFILES_ACTIVE=prod

# Keycloak
KEYCLOAK_DB=keycloak
KEYCLOAK_USER=keycloak
KEYCLOAK_PASSWORD=password
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# Match Service PostgreSQL
POSTGRES_MATCH_DB=match_db
POSTGRES_MATCH_USER=match_user
POSTGRES_MATCH_PASSWORD=match_pass

# User Service PostgreSQL
POSTGRES_USER_DB=user_db
POSTGRES_USER_USER=user_user
POSTGRES_USER_PASSWORD=user_pass

HOSTNAME=host.docker.internal